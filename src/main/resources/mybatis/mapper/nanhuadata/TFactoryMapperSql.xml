<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sqltest.mapper.nanhuadata.TFactoryMapper">

    <select id="getString" resultType="java.lang.String">
        SELECT
        Name
        FROM
        TFactory
        WHERE
        code = #{code}
    </select>

    <select id="getEntiyList" resultMap="BaseResultMap">
        SELECT
        *
        FROM
        TFactory

    </select>

</mapper>